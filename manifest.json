{

  "manifest_version": 2,
  "name": "Context Tablist",
  "description": "Context Tablist",
  "version": "0.3",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "56.0a1"
    }
  },

  "icons": {
  "16": "logos/menu_open-24px.svg",
  "64": "logos/menu_open-24px.svg"
  },

   "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "menus",
    "activeTab",
    "tabs"
  ]
  
}
